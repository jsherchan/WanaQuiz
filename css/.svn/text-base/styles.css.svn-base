/* CSS Document */
body{
	background-color:#E6F2D2;
	font-family:Tahoma, Geneva, sans-serif, Arial;
	font-size:12px;
	color:#000;
	margin:0px;
	padding:0px;
	/*behavior: url("csshover3.htc"); */
}

@font-face {
	font-family: 'rupee';
	src: url('rupee-webfont.eot');
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-style: normal;
}

.rs {
    font-family:rupee;
}


/*img, a, div{ behavior: url(iepngfix.htc) }*/
:focus {
	outline: 0;
}
a{
	text-decoration:none;
	color:#0066CC; 
}
a:hover{
	text-decoration:underline;
}
a img{
	margin:0px;
	padding:0px;
	border:none;
}
p{
	margin:0px;
	padding:0px;
}

h1,h2,h3,h4{
	margin:0px;
	padding:0px;
}
ul{
	margin:0px 0px 0px 0px;
	padding:0px 0px 0px 0px;
}

ul li{
	margin:0px 0px 0px 0px;
	padding:0px 0px 0px 0px;
}


.clear{
	clear:both;
}

.italic{ font-style:italic; }
.bold{ font-weight:bold; }

.text_right{ text-align:right; }
.text_center{ text-align:center; }

.color_blue{ color:#0066CC; } 
.color_darkblue{ color:#003366; }
.color_lightblue{ color:#00B7FC; }
.color_white{ color:#FFF; }
.color_gray{ color:#555; }
.color_orange{ color:#EB301F; }
.color_green{ color:#6DAA00; }
.color_darkbrown{ color:#7F0000; }

.font10{ font-size:10px; }
.font11{ font-size:11px; }
.font13{ font-size:13px; }
.font14{ font-size:14px; }
.font16{ font-size:16px; }
.font20{ font-size:20px; }
.font24{ font-size:24px; }

.padding_5top{ padding-top:5px; }
.padding_5bottom{ padding-bottom:5px; }
.padding_10top{ padding-top:10px; }
.padding_10topbottom{ padding:10px 0; }
.padding_5topbottom{ padding:5px 0; }
.padding_2bottom{ padding-bottom:2px; }
.padding_10leftright{ padding:0 10px; }

.desc{}
.desc h4{ font-size:16px; font-weight:bold; }
.desc p{ padding:5px 0; }

.desc_help{ font-family:Tahoma, Geneva, sans-serif, Arial; font-size:12px; }
.desc_help h3{ font-size:1.17em; }
.desc_help p{ margin:1em 0; line-height:20px; }
.desc_help ul{ margin:15px 0 15px 20px; padding:0px 0 0 20px; }
.desc_help ul li{ list-style:disc; list-style-position:outside; line-height:20px; }
.desc_help table tr td{ padding:2px; font-size:12px; }
.desc_help table { border-collapse: collapse; border-style: none;}
/*.desc_help p a{ color:#000; text-decoration:none; }
.desc_help p strong a{ color:#0066CC; }*/
.desc_help a:hover{ text-decoration:underline; }
body a.anchor_href{ color:#000; text-decoration:none}
a.anchor_href:hover{text-decoration:none !important}